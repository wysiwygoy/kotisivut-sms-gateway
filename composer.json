{
    "name": "laravel-notification-channels/kotisivut-sms-gateway",
    "description": "Laravel notification channel for Kotisivut SMS-Gateway.",
    "homepage": "https://github.com/laravel-notification-channels/kotisivut-sms-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Jarno Antikainen / Wysiwyg Oy",
            "email": "jarno.antikainen@wysiwyg.fi",
            "homepage": "https://www.wysiwyg.fi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "6.*",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\KotisivutSmsGateway\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\KotisivutSmsGateway\\Test\\": "tests",
            "NotificationChannels\\KotisivutSmsGateway\\IntegrationTest\\": "tests-integration"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
